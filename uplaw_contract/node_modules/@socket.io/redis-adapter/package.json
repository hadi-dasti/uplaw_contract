    "socket.io-adapter": "^2.4.0"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.7",
    "expect.js": "0.3.1",
    "ioredis": "^5.3.2",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.7",
    "redis": "^4.6.6",
    "redis-v3": "npm:redis@^3.1.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
