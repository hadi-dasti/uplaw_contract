    "format:fix": "prettier --parser typescript --write 'lib/**/*.ts' 'test/**/*.ts'",
    "prepack": "tsc"
  },
  "dependencies": {
    "debug": "~4.3.1",
    "notepack.io": "~3.0.1",
    "uid2": "1.0.0"
